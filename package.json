{
  "name": "daily-diary",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "dev:backend": "nodemon server.js",
    "dev:frontend": "cd frontend && next dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "seed": "node scripts/seed.js",
    "start": "node server.js",
    "build": "cd frontend && next build",
    "start:frontend": "cd frontend && next start"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "next": "^14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "concurrently": "^8.2.0"
  }
}
